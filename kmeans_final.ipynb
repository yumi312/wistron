    "        dist = round(distance.euclidean(point, center), 5)\n",
    "        one_dot_dist_list.append(dist)\n",
    "    final_dist_list.append(one_dot_dist_list)\n",
    "\n",
    "final_dist_list = pd.DataFrame(final_dist_list)\n",
    "X_all_df[\"dist_A\"] = final_dist_list[0]\n",
    "X_all_df[\"dist_B\"] = final_dist_list[1]\n",
    "\n",
    "predict = []\n",
    "groups = []\n",
    "for k in range(X_all_df.shape[0]):\n",
    "    one_dot_list = []\n",
    "    if X_all_df.iloc[k].dist_A <= f1_df.iloc[0].A_radius:\n",
    "        one_dot_list.append(\"A\")\n",
    "        \n",
    "    if X_all_df.iloc[k].dist_B <= f1_df.iloc[0].B_radius:\n",
    "        one_dot_list.append(\"B\")\n",
    "    \n",
    "    groups.append(one_dot_list)\n",
    "    predict.append(leave_tag if len(one_dot_list) > 0 else stay_tag)\n",
    "X_all_df[\"groups\"] = groups\n",
    "X_all_df[\"predict\"] = predict\n",
