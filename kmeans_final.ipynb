   "source": [
    "查找最適合的有效半徑，大多使用半徑去依比例配比標準差<br>\n",
    "ex: mean - 1std   mean - 0.5std"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 23,
   "metadata": {},
   "outputs": [],
   "source": [
    "f1_df = pd.DataFrame(columns = [\"f1\", \"A_radius\", \"B_radius\"])\n",
    "for a in np.arange(-1, 1, 0.5):\n",
    "    A_radius = cluster_df.iloc[0][\"mean\"] - round(a, 2) * cluster_df.iloc[0][\"std\"]\n",
    "    for b in np.arange(-1, 1, 0.5):\n",
    "        B_radius = cluster_df.iloc[1][\"mean\"] - round(b, 2) * cluster_df.iloc[1][\"std\"]   \n",
    "\n",
    "        predict = []\n",
    "        groups = []\n",
    "        for k in range(circle.shape[0]):\n",
    "            one_dot_list = []\n",
    "            if circle.iloc[k].dist_A <= A_radius:\n",
    "                one_dot_list.append(\"A\")\n",
    "            if circle.iloc[k].dist_B <= B_radius:\n",
    "                one_dot_list.append(\"B\")\n",
    "            # if circle.iloc[k].dist_C <= C_radius:\n",
    "            #     one_dot_list.append(\"C\")\n",
    "            groups.append(one_dot_list)\n",
    "            predict.append(leave_tag if len(one_dot_list) > 0 else stay_tag)\n",
    "        circle[\"groups\"] = groups\n",
    "        circle[\"predict\"] = predict\n",
    "        \n",
    "        score = accuracy_score(circle[\"origin\"], circle[\"predict\"])\n",
    "        score_df = pd.DataFrame({\"acc\":[score], \"A_radius\":[A_radius], \"B_radius\":[B_radius]})\n",
    "        acc_df = pd.concat([acc_df, score_df])\n",
    "        acc_df = acc_df.sort_values(\"acc\", ascending=False)"
   ]
  },
